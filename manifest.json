{
  "name": "Redirect New Tab",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Redirect the new tab page to the page of your choice.",
  "homepage_url": "http://www.fluxbytes.com",
  "icons": {
    "16": "icons/redirect_new_tab_18x.png",
    "48": "icons/redirect_new_tab_46x.png",
    "128":"icons/redirect_new_tab_128x.png"
  },
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "options_page": "src/options/index.html",
  "permissions": [
    "tabs",
    "storage"
  ]
}